{
  "name": "@thingweb/ui-wot-components",
  "version": "0.1.0",
  "description": "Independent UI components library to build interfaces for IoT applications",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui-wot-components/ui-wot-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs.js"
    },
    "./components": {
      "import": "./dist/components.js",
      "types": "./dist/types/components.d.ts",
      "require": "./dist/components.cjs.js"
    },
    "./services": {
      "import": "./dist/services.js", 
      "types": "./dist/types/services.d.ts",
      "require": "./dist/services.cjs.js"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/types/utils.d.ts", 
      "require": "./dist/utils.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eclipse-thingweb/ui-wot.git"
  },
  "keywords": [
    "ui-components",
    "iot",
    "dashboard",
    "web-of-things",
    "wot",
    "thing-description",
    "web-components",
    "stencil",
    "typescript"
  ],
  "author": "Eclipse Thingweb <thingweb-dev@eclipse.org> (https://thingweb.io/)",
  "license": "EPL-2.0 OR W3C-20150513",
  "bugs": {
    "url": "https://github.com/eclipse-thingweb/ui-wot/issues"
  },
  "homepage": "https://github.com/eclipse-thingweb/ui-wot/packages/components#readme",
  "files": [
    "dist/",
    "loader/",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
  "build": "stencil build",
  "start": "node ./dev-serve.js",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "docs": "npx typedoc --options typedoc.json"
  },
  "devDependencies": {
    "@stencil-community/postcss": "2.2.0",
    "@stencil/core": "^4.27.1",
    "@types/aria-query": "^5.0.4",
    "@types/chai": "^5.2.2",
    "@types/deep-eql": "^4.0.2",
    "@types/estree": "^1.0.8",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.13.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/use-sync-external-store": "^1.5.0",
    "autoprefixer": "10.4.21",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "postcss-import": "16.1.1",
    "puppeteer": "^24.3.0",
  "tailwindcss": "3.4.17",
  "typedoc": "0.29.0"
  }
}
